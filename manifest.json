    "permissions": ["storage", "tabs", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://*.imdb.com/*"],
        "js":  ["zip.min.js"]
      }
    ]
  }